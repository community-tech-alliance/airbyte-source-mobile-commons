{
    "streams": [
        {
            "stream": {
                "name": "tiny_urls",
                "supported_sync_modes": [
                    "full_refresh",
                    "incremental"
                ],
                "json_schema": {
                    "$schema": "http://json-schema.org/draft-07/schema#",
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "string"
                          },
                        "created_at": {
                            "type": ["string", "null"]
                        },
                        "name": {
                            "type": ["string", "null"]
                        },
                        "mode": {
                            "type": ["string", "null"]
                        },
                        "url": {
                            "type": ["string", "null"]
                        },
                        "host": {
                            "type": ["string", "null"]
                        },
                        "description": {
                            "type": ["string", "null"]
                        },
                        "key": {
                            "type": ["string", "null"]
                        }
                    }
                }
            },
            "sync_mode": "incremental",
            "destination_sync_mode": "overwrite"
        }
    ]
}